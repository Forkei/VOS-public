# Browser Agent Environment Configuration

# RabbitMQ Configuration
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=vos_user
RABBITMQ_PASSWORD=your_rabbitmq_password
RABBITMQ_VHOST=vos_vhost

# PostgreSQL Configuration
DATABASE_HOST=postgres
DATABASE_PORT=5432
DATABASE_NAME=vos_database
DATABASE_USER=vos_user
DATABASE_PASSWORD=your_postgres_password

# API Gateway Configuration
API_GATEWAY_HOST=api_gateway
API_GATEWAY_PORT=8000

# Weaviate Configuration
WEAVIATE_HOST=weaviate
WEAVIATE_PORT=8080

# Gemini API Key (required for browser-use LLM)
GEMINI_API_KEY=your_gemini_api_key_here

# Agent Configuration
AGENT_NAME=browser_agent
AGENT_DISPLAY_NAME=Browser Automation Service

# Message History Limits
MAX_CONVERSATION_MESSAGES=0
BROWSER_AGENT_MAX_CONVERSATION_MESSAGES=15

# Environment
ENVIRONMENT=development

# Sentry DSN (optional)
SENTRY_DSN=your_sentry_dsn_here

# Logging Level
LOG_LEVEL=INFO
